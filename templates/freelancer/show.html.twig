

{% block title %}Freelancer{% endblock %}

{% block body %}
    <h1>Freelancer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ freelancer.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ freelancer.name }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ freelancer.lastname }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ freelancer.email }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_freelancer_index') }}">back to list</a>

    <a href="{{ path('app_freelancer_edit', {'id': freelancer.id}) }}">edit</a>

    {{ include('freelancer/_delete_form.html.twig') }}
{% endblock %}
