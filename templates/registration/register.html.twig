<body>



  <main id="main" class="main">


    <section class="section">
      <div class="row">
        <div class="col-lg-6">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Sign up</h5>

              <!-- General Form Elements -->
{% block body %}
    {{ form_start(registrationForm) }}
         <div style="display: flex; justify-content: space-between;padding-bottom:15px">
            <div style="flex: 1; margin-right: 10px;">
                <label for="inputText" class="col-form-label">First Name</label>
                {{ form_widget(registrationForm.FirstName) }}
            </div>
            <div style="flex: 1; margin-left: 10px;">
                <label for="inputText" class="col-form-label">Last Name</label>
                {{ form_widget(registrationForm.LastName) }}
            </div>
        </div>
                <div class="row mb-3">
                  <label for="inputEmail" class="col-sm-2 col-form-label">Email</label>
                  <div class="col-sm-10">
        {{ form_widget(registrationForm.email) }}
            {{ form_errors(registrationForm.email) }}


                  </div>
                </div>
                <div class="row mb-3">
                  <label for="inputPassword" class="col-sm-2 col-form-label">Password</label>
                  <div class="col-sm-10">
                          {{ form_widget(registrationForm.plainPassword, {
                              label: 'Password'
        }) }}
                  </div>
                </div>
     
                <fieldset class="row mb-3">
                  <legend class="col-form-label col-sm-2 pt-0"></legend>
                  <div class="col-sm-10">
                    <div class="form-check">
        {{ form_widget(registrationForm.agreeTerms) }}
                      <label class="form-check-label" for="gridRadios1">
                      </label>
                    </div>
          

                 <div class="row mb-3">
            <label class="col-sm-2 col-form-label"> </label>
            <div class="col-sm-10">
                {{ form_widget(registrationForm.save) }}
            </div>
<p>Already have an account? <a href="{{ path('app_login') }}">Log In</a></p>

        </div>
    {{ form_end(registrationForm) }}
{% endblock %}	
            </div>

          </div>

        </div>

       
      </div>

    </section>
</form>
  </main><!-- End #main -->

  
  <!-- Vendor JS Files -->
  <script src="assets/vendor/apexcharts/apexcharts.min.js"></script>
  <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="assets/vendor/chart.js/chart.umd.js"></script>
  <script src="assets/vendor/echarts/echarts.min.js"></script>
  <script src="assets/vendor/quill/quill.min.js"></script>
  <script src="assets/vendor/simple-datatables/simple-datatables.js"></script>
  <script src="assets/vendor/tinymce/tinymce.min.js"></script>
  <script src="assets/vendor/php-email-form/validate.js"></script>

  <!-- Template Main JS File -->
  <script src="assets/js/main.js"></script>

</body>

</html>